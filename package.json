{
  "name": "app-backend",
  "version": "1.0.0",
  "description": "Backend for APP: Node.js &amp; TypeScript, Postgres via Prisma, JWT auth. Provides endpoints for auth, archetypes, mood &amp; IFS check‑ins, loved‑one letters, journaling &amp; gratitude rituals, triggers, community messages &amp; push tokens. Supports sacred archetypes &amp; trauma‑informed interventions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "postgres",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/node-fetch": "^2.6.13",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
